{
  "name": "@codemieai/code",
  "version": "0.0.13",
  "description": "Unified AI coding assistant CLI - Manage Claude Code, Codex, Gemini & custom agents. Multi-provider support (OpenAI, Azure, LiteLLM, SSO). Built-in LangGraph agent with file operations & git integration.",
  "type": "module",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "bin": {
    "codemie": "./bin/codemie.js",
    "codemie-code": "./bin/agent-executor.js",
    "codemie-claude": "./bin/agent-executor.js",
    "codemie-codex": "./bin/agent-executor.js",
    "codemie-gemini": "./bin/agent-executor.js",
    "codemie-deepagents": "./bin/agent-executor.js"
  },
  "files": [
    "dist",
    "bin",
    "src/workflows/templates/**/*.yml",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "build": "tsc",
    "dev": "tsc --watch",
    "test": "vitest",
    "test:unit": "vitest run src",
    "test:integration": "vitest run tests/integration",
    "test:coverage": "vitest run --coverage",
    "test:watch": "vitest --watch",
    "test:ui": "vitest --ui",
    "test:run": "vitest run",
    "lint": "eslint 'src/**/*.ts' --max-warnings=0",
    "lint:fix": "eslint 'src/**/*.ts' --fix",
    "validate:commit": "python3 .github/scripts/validate_commit.py --pattern '^(feat|fix|docs|style|refactor|perf|test|chore|ci)(\\([a-z-]+\\))?!?: [a-z].*$' --max-length 72 --verbose",
    "validate:secrets": "docker run --rm -v \"$(pwd):/path\" ghcr.io/gitleaks/gitleaks:latest detect --source=\"/path\" --verbose --no-git",
    "license-check": "npx license-checker --summary --onlyAllow 'MIT;Apache-2.0;ISC;BSD-2-Clause;BSD-3-Clause;0BSD;CC0-1.0;Unlicense;BlueOak-1.0.0;Python-2.0;CC-BY-4.0;Apache*;(MIT OR CC0-1.0)'",
    "ci": "npm run validate:commit && npm run license-check && npm run lint && npm run build && npm run test:unit && npm run test:integration",
    "ci:full": "npm run validate:commit && npm run validate:secrets && npm run license-check && npm run lint && npm run build && npm run test:unit && npm run test:integration",
    "prepublishOnly": "npm run build",
    "prepack": "npm run build"
  },
  "keywords": [
    "ai",
    "artificial-intelligence",
    "coding-assistant",
    "code-generation",
    "cli",
    "command-line",
    "claude",
    "claude-code",
    "openai",
    "codex",
    "gemini",
    "anthropic",
    "gpt",
    "chatgpt",
    "ai-agents",
    "coding-agents",
    "langgraph",
    "langchain",
    "code-review",
    "refactoring",
    "test-generation",
    "documentation",
    "bug-fixing",
    "litellm",
    "azure-openai",
    "aws-bedrock",
    "sso",
    "enterprise",
    "multi-provider",
    "developer-tools",
    "productivity",
    "automation",
    "pair-programming",
    "code-assistant",
    "ai-code",
    "copilot-alternative",
    "cursor-alternative"
  ],
  "author": "AI/Run Team",
  "license": "Apache-2.0",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/codemie-ai/codemie-code.git"
  },
  "homepage": "https://github.com/codemie-ai/codemie-code#readme",
  "bugs": {
    "url": "https://github.com/codemie-ai/codemie-code/issues"
  },
  "dependencies": {
    "@clack/core": "^0.5.0",
    "@clack/prompts": "^0.11.0",
    "@langchain/core": "^1.0.4",
    "@langchain/langgraph": "^1.0.2",
    "@langchain/openai": "^1.1.0",
    "chalk": "^5.3.0",
    "cli-table3": "^0.6.5",
    "commander": "^11.1.0",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^5.1.0",
    "inquirer": "^9.2.12",
    "keytar": "^7.9.0",
    "open": "^8.4.2",
    "ora": "^7.0.1",
    "zod": "^4.1.12"
  },
  "devDependencies": {
    "@eslint/js": "^9.38.0",
    "@types/cors": "^2.8.19",
    "@types/express": "^5.0.5",
    "@types/inquirer": "^9.0.7",
    "@types/node": "^20.10.5",
    "@typescript-eslint/eslint-plugin": "^8.46.2",
    "@typescript-eslint/parser": "^8.46.2",
    "@vitest/ui": "^4.0.10",
    "eslint": "^9.38.0",
    "typescript": "^5.3.3",
    "vitest": "^4.0.10"
  },
  "engines": {
    "node": ">=24.0.0"
  }
}
